create table Rummyregister(USER_ID INT NOT NULL AUTO_INCREMENT, USERNAME VARCHAR(20), PASSWORD VARCHAR(20), EMAILID VARCHAR(30), MOBILE INT, PRIMARY KEY(USER_ID));

create table rummyregister(USER_ID INT NOT NULL AUTO_INCREMENT, USERNAME VARCHAR(20), PASSWORD VARCHAR(20), EMAILID VARCHAR(30), MOBILE VARCHAR(15), PRIMARY KEY(USER_ID));

