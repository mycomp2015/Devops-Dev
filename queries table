create table playerdetail(REGISTERDATE DATE,GAMERLEVEL VARCHAR(20),
USER_ID INT NOT NULL,GAMESPLAYED VARCHAR(20), FOREIGN KEY fk_USERID(USER_ID) REFERENCES Rummyregister(USER_ID));

Insert into playerdetails (REGISTERDATE, GAMERLEVEL, USER_ID, GAMESPLAYED) values ('18-06-10','beginner','5','chess');

create table student(ROLLNO INT NOT NULL AUTO_INCREMENT, NAME VARCHAR(20),PASSWORD VARCHAR(20), EMAILID VARCHAR(30),SUB1 VARCHAR(20),SUB2 VARCHAR(20),MOBILE VARCHAR(15), PRIMARY KEY(ROLLNO));

