Create table cointype(COINTYPEID int NOT NULL AUTO_INCREMENT,DECSRIPTION VARCHAR(20),PRIMARY KEY(COINTYPEID));
 

ALTER TABLE user ALTER COLUMN ACCNTSTATUS SET DEFAULT '0';


create table user(USERID int not null auto_increment,USERNAME VARCHAR(20) not null,CONTACTNO bigint not null, ADDR VARCHAR (50), ACCNTSTATUS INT, PWD VARCHAR(20), REGTIME datetime,LASTLOGIN datetime, PRIMARY KEY(USERID));